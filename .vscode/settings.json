      {
        // Move down
        "before": ["<A-j>"],
        "commands": ["editor.action.moveLinesDownAction"]
      },
      {
        // Move up
        "before": ["<A-k>"],
        "commands": ["editor.action.moveLinesUpAction"]
      },
      {
        // Prev buffer
        "before": ["<S-h>"],
        "commands": [":bprev"]
      },
      {
        // Next buffer
        "before": ["<S-l>"],
        "commands": [":bnext"]
      },
      {
        // Prev buffer
        "before": ["[", "b"],
        "commands": [":bprev"]
      },
      {
        // Next buffer
        "before": ["]", "b"],
        "commands": [":bnext"]
